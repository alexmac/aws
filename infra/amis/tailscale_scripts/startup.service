################################################################################
# startup.service
################################################################################

[Unit]
Before=ssh.service
Wants=network.target
After=network.target
Description=Runs startup.sh
  
[Service]
Type=simple
User=root
Group=root
ExecStart=/usr/local/ami_setup/tailscale_scripts/startup.sh
Restart=on-failure


[Install]
WantedBy=multi-user.target
